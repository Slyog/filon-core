{
  "name": "noion-core",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "dev": "next dev",
    "dev:timeout": "node scripts/dev-timeout.js",
    "build": "next build --webpack",
    "start": "next start",
    "save-report": "node scripts/save-report.mjs",
    "qa-viewer": "node scripts/qa-viewer.mjs",
    "lint": "eslint",
    "test": "jest --config jest.config.ts",
    "test:a11y": "jest --config jest.config.ts src/__tests__/a11y",
    "test:watch": "jest --watch --config jest.config.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:performance": "playwright test e2e/performance.spec.ts",
    "qa:dev": "next dev -p 3000",
    "qa:test": "playwright test",
    "qa:all": "playwright test --project=chromium --reporter=list",
    "qa:glow": "playwright test tests/__snapshots__/node-feedback/qa-node-feedback-snapshot.spec.ts",
    "qa:ui": "playwright test tests/*ui*.spec.ts",
    "qa:fast": "playwright test --reporter=list",
    "qa:save": "npm run save-report",
    "qa:view": "npm run qa-viewer",
    "qa:metrics": "node scripts/generate-badge.mjs",
    "qa:layout": "npx playwright test tests/qa-layout-autofix.spec.ts",
    "qa:layout:lock": "npx playwright test tests/qa-layout-lock.spec.ts",
    "qa:layout:update": "node scripts/update-layout-baseline.mjs",
    "qa:report": "npm run qa:test && npm run qa:metrics && npm run qa:layout",
    "qa:headed": "npx playwright test --headed",
    "qa:axe": "jest --runTestsByPath src/__tests__/qa-unified-shell.spec.ts",
    "db:reset": "ts-node prisma/reset.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@automerge/automerge": "^3.2.0",
    "@emotion/is-prop-valid": "^1.4.0",
    "@prisma/client": "^6.18.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-slot": "^1.2.4",
    "ai": "^3.4.33",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dexie": "^4.2.1",
    "dotenv": "^17.2.3",
    "framer-motion": "^12.23.24",
    "localforage": "^1.10.0",
    "lucide-react": "^0.469.0",
    "next": "16.0.1",
    "next-themes": "^0.4.6",
    "noion-proprietary": "file:../noion-proprietary",
    "openai": "^4.55.4",
    "prisma": "^6.18.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-markdown": "^10.1.0",
    "react-virtuoso": "^4.14.1",
    "reactflow": "^11.11.4",
    "recharts": "^2.15.4",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.76",
    "zustand": "^5.0.8",
    "filon-proprietary": "workspace:*"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@axe-core/react": "^4.11.0",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/testing-library__jest-dom": "^6.0.0",
    "autoprefixer": "^10.4.21",
    "axe-core": "^4.11.0",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-playwright-preset": "^4.0.0",
    "playwright": "^1.56.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.16",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "wait-on": "^7.2.0"
  },
  "license": "GPL-3.0"
}
