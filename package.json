{
  "name": "noion-core",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "dev": "next dev",
    "dev:timeout": "node scripts/dev-timeout.js",
    "build": "next build --webpack",
    "start": "next start",
    "save-report": "node scripts/save-report.mjs",
    "qa-viewer": "node scripts/qa-viewer.mjs",
    "lint": "eslint",
    "test": "jest --config jest.config.ts",
    "test:a11y": "jest --config jest.config.ts src/__tests__/a11y",
    "test:watch": "jest --watch --config jest.config.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:performance": "playwright test e2e/performance.spec.ts",
    "qa:dev": "next dev -p 3000",
    "qa:test": "PW_HEADLESS=1 playwright test",
    "qa:intent-router": "PW_HEADLESS=1 playwright test tests/qa-intent-router.spec.ts --project=chromium",
    "qa:context-stream": "PW_HEADLESS=1 playwright test tests/qa-context-stream.spec.ts --project=chromium",
    "qa:toolchain": "PW_HEADLESS=1 playwright test tests/qa-graph-toolchain.spec.ts --project=chromium",
    "qa:toolchain:retry": "PW_HEADLESS=1 playwright test tests/qa-toolchain-retry.spec.ts --project=chromium",
    "qa:meta-tracking": "PW_HEADLESS=1 playwright test tests/qa-meta-tracking.spec.ts --project=chromium",
    "qa:node-feedback": "PW_HEADLESS=1 playwright test tests/qa-node-feedback.spec.ts --project=chromium",
    "qa:all": "PW_HEADLESS=1 playwright test --project=chromium",
    "qa:mock": "cross-env NODE_ENV=test FILON_QA_MODE=mock PW_HEADLESS=1 npm run qa:all && cross-env NODE_ENV=test FILON_QA_MODE=mock node scripts/save-report.mjs --mode=mock",
    "qa:real": "cross-env NODE_ENV=production FILON_QA_MODE=real PW_HEADLESS=1 npm run qa:all && cross-env NODE_ENV=production FILON_QA_MODE=real node scripts/save-report.mjs --mode=real",
    "qa:mixed": "cross-env NODE_ENV=test FILON_QA_MODE=mixed PW_HEADLESS=1 npm run qa:all && cross-env NODE_ENV=test FILON_QA_MODE=mixed node scripts/save-report.mjs --mode=mixed",
    "qa:sweep": "npm run qa:mock && npm run qa:real && npm run qa:mixed && npm run qa:save-report",
    "qa:save-report": "node scripts/save-report.mjs",
    "qa:heal": "PW_HEADLESS=1 node scripts/qa-heal.mjs",
    "qa:snap": "PW_HEADLESS=1 node scripts/regen-snapshots.mjs",
    "qa:save": "npm run save-report",
    "qa:view": "npm run qa-viewer",
    "qa:metrics": "node scripts/generate-badge.mjs",
    "qa:layout": "PW_HEADLESS=1 playwright test tests/qa-layout-autofix.spec.ts",
    "qa:layout:lock": "PW_HEADLESS=1 playwright test tests/qa-layout-lock.spec.ts",
    "qa:layout:update": "node scripts/update-layout-baseline.mjs",
    "qa:update-dashboard": "node scripts/update-dashboard.mjs",
    "qa:report": "npm run qa:test && npm run qa:metrics && npm run qa:layout",
    "qa:axe": "jest --runTestsByPath src/__tests__/qa-unified-shell.spec.ts",
    "db:reset": "ts-node prisma/reset.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@automerge/automerge": "^3.2.0",
    "@emotion/is-prop-valid": "^1.4.0",
    "@prisma/client": "^6.18.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-slot": "^1.2.4",
    "ai": "^3.4.33",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dexie": "^4.2.1",
    "dotenv": "^17.2.3",
    "framer-motion": "^12.23.24",
    "localforage": "^1.10.0",
    "lucide-react": "^0.469.0",
    "next": "16.0.1",
    "next-themes": "^0.4.6",
    "noion-proprietary": "file:../noion-proprietary",
    "openai": "^4.55.4",
    "prisma": "^6.18.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-markdown": "^10.1.0",
    "react-virtuoso": "^4.14.1",
    "reactflow": "^11.11.4",
    "recharts": "^2.15.4",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.76",
    "zustand": "^5.0.8",
    "filon-proprietary": "file:stubs/filon-proprietary"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@axe-core/react": "^4.11.0",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/testing-library__jest-dom": "^6.0.0",
    "autoprefixer": "^10.4.21",
    "axe-core": "^4.11.0",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-playwright-preset": "^4.0.0",
    "playwright": "^1.56.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.16",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "wait-on": "^7.2.0",
    "cross-env": "^7.0.3"
  },
  "license": "GPL-3.0"
}
